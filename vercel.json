{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "!(api)/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/server.js"
      }
    ]
  }